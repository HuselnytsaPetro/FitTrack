    "test:open": "cypress open",
    "test:ci": "cypress run --browser chrome --headless"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "lucide-react": "^0.546.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^6.14.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "@cypress/code-coverage": "^3.12.43",
    "cypress": "^15.5.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.9",
    "nyc": "^15.1.0",
    "prettier": "^3.3.3",
    "vite-plugin-istanbul": "^7.0.0",
    "vite": "^7.1.7"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
